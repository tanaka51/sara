= form_for(@wiki) do |f|
  - if @wiki.errors.any?
    #error_explanation
      %h2
        = pluralize(@wiki.errors.count, "error")
        prohibited this wiki from being saved:
      %ul
        - @wiki.errors.full_messages.each do |message|
          %li= message
  .field
    = f.label :blob
    %br/
    = f.text_field :blob
  .field
    = f.label :name
    %br/
    = f.text_field :name
  .actions
    = f.submit
